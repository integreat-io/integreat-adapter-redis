{
  "name": "integreat-adapter-redis",
  "version": "0.3.1",
  "description": "Redis adapter for Integreat",
  "author": "Kjell-Morten Bratsberg Thorsen <post@kjellmorten.no>",
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "tsc && node dist/index.js",
    "test": "npm run lint && npm run build && nyc --reporter=text-summary ava ./dist/**/*.test.js",
    "test:watch": "npm run dev",
    "test:inspect": "node --inspect node_modules/ava/profile.js",
    "dev": "ava --watch",
    "build": "tsc",
    "prepare": "npm run build",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "nyc:report": "nyc report",
    "lint": "tslint --project tsconfig.json --config tslint.json"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ],
    "files": [
      "src/**/*.test.ts"
    ],
    "sources": [
      "src/**/!(*.test).ts"
    ]
  },
  "nyc": {
    "extension": [
      ".ts",
      ".js"
    ],
    "include": [
      "**/!(*.test).js",
      "**/!(*.test|*.d).ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/integreat-io/integreat-adapter-redis.git"
  },
  "bugs": {
    "url": "https://github.com/integreat-io/integreat-adapter-redis/issues"
  },
  "homepage": "https://github.com/integreat-io/integreat-adapter-redis#readme",
  "engines": {
    "node": ">= 8.6",
    "npm": ">= 5.4"
  },
  "dependencies": {
    "@types/debug": "^4.1.5",
    "@types/ramda": "^0.27.6",
    "@types/redis": "^2.8.22",
    "debug": "^4.1.1",
    "map-any": "^0.2.1",
    "p-limit": "^2.3.0",
    "ramda": "^0.27.0",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@types/sinon": "^9.0.4",
    "ava": "^2.4.0",
    "coveralls": "^3.1.0",
    "nyc": "^15.1.0",
    "sinon": "^9.0.2",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.7.3"
  }
}
